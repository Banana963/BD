FOREIGN KEY (nr_jogo) REFERENCES F33_jogo (nr_jogo)
)Engine InnoDB;

CREATE TABLE F33_jogador_participante
(nr_camisola	INTEGER								NOT NULL,
FOREIGN KEY (nr_camisola) REFERENCES F33_jogador (nr_camisola),
