)Engine InnoDB;

CREATE TABLE F33_Dirigente
(id_dirigente	INTEGER		auto_increment			PRIMARY KEY,
id_equipa		INTEGER 							NOT NULL,
FOREIGN KEY (id_equipa) REFERENCES F33_equipa (id)
)Engine InnoDB;

