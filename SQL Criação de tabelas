FOREIGN KEY (id_equipa) REFERENCES F33_equipa (id)
)Engine innodb;

CREATE TABLE F33_jogo
(nr_jogo		INTEGER		auto_increment			PRIMARY KEY,
nome			varchar(20)							NOT NULL,
